{  "compilerOptions": {    "jsx": "react-jsx",    "target": "es5",    "lib": ["dom", "es6"],    "allowJs": true,    "skipLibCheck": true,    "esModuleInterop": true,    "allowSyntheticDefaultImports": true,    "strict": true,    "forceConsistentCasingInFileNames": true,    "module": "esnext",    "moduleResolution": "node",    "resolveJsonModule": true,    "isolatedModules": true  },  "include": [    "src"  ],  "exclude": [    "node_modules"  ]}